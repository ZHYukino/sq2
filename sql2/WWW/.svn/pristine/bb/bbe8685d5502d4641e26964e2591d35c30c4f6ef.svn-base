<!--#include file="../comm/mconn.asp"-->
<%
	Session.CodePage=65001
	Response.Charset="UTF-8"
	itype = Request.QueryString("itype")

	if itype = 1 then
		Dim mRs
		Set mRs=Server.CreateObject("Adodb.RecordSet")
		mRs.open "Select * from DevType where FEnable=1", conn, 1,2
		response.write "{'results': " & mRs.recordcount
		if not mRs.eof then
			response.write  ",'rows': ["
			do while not mRs.eof
				response.write "{'fid':" & mRs("FID") & ", 'fcname': '" & mRs("FCName") & "', 'fename': '" & mRs("FEName") &"'}"
				mRs.movenext
				if not mRs.eof then
					response.write ","
				end if				
			loop
			response.write "]"
		end if
		response.write "}"
		mRs.close
	end if
	
%>
