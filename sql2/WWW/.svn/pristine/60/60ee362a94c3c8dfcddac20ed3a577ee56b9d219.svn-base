	Ext.getCmp('cms_startymd').setValue(b1);
	Ext.getCmp('cms_endymd').setValue(b2);
	Ext.getCmp('cms_dateym').setValue(b3);
	Ext.getCmp('cms_datey').setValue(b4);
}

