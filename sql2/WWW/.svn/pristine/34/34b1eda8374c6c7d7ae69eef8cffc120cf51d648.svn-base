function loadCheckbox(){
	$.ajax({
		type: "GET",
		url : "json/getcheck.asp?itype=1&dc=" + new Date().getTime() + "",
		//data:{},
		dataType: "json",
		success: function(data){
			//alert(data.TS)
			
			for(var dataitem in data){
				//alert(data[dataitem])
				
				if(data[dataitem]==1){
					$("input[typename="+ dataitem +"]").attr("checked","");
				}
			}
			
			//赋值加载图
			selectTunnel();
		 },
		error: function(json){}
	});
}

